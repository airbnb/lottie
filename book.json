
{
  "gitbook": "3.2.3",
  "title": "Lottie",
  "description": "After Effects Animations for Android, iOS, React Native, Web, and Windows",
  "root": "./docs",
  "structure": {
    "summary": "SUMMARY.md"
  },
  "variables": {
    "androidVersion": "2.5.1",
    "iosVersion": "2.5.0",
    "reactNativeVersion": "2.3.2"
  },
  "plugins": [
    "edit-link",
    "prism@2.3.0",
    "-highlight",
    "github",
    "-search",
    "codeblock-disable-glossary",
    "collapsible-chapters",
    "anchors"
  ],
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/airbnb/lottie/edit/lottie-docs/docs/",
      "label": "Edit This Page"
    },
    "github": {
      "url": "https://github.com/airbnb/lottie/"
    },
    "collapsible-chapters": {}
  }
}
